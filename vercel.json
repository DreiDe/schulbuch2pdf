{
  "version": 2,
  "name": "nodejs-mysql",
  "builds": [
    { "src": "server/server.js", "use": "@vercel/node" },
    { "src": "server/public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "socket.io/(.*)", "dest": "server/server.js" },
    { "src": "download/(.*)", "dest": "server/server.js" },
    { "src": "/", "dest": "server/public/index.html" },
    { "src": "/(.+)", "dest": "server/public/$1" }
  ]
}
